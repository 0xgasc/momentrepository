    "solidity-coverage": "^0.8.16",
    "tailwindcss": "^4.1.10",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^4.9.5",
    "bundlesize": "^0.18.1",
    "lighthouse": "^11.7.0",
    "@lhci/cli": "^0.13.0",
    "serve": "^14.2.1"
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "500 kB",
      "compression": "gzip"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "50 kB",
      "compression": "gzip"
    }
  ]
