# Copy this to .env and fill in your actual values
# Never commit .env to git - add it to .gitignore

# API Keys
SETLIST_FM_API_KEY=your-setlist-fm-api-key-here

# Database
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Authentication
JWT_SECRET=generate-strong-secret-with-openssl-rand-base64-32

# Blockchain (DO NOT COMMIT REAL KEYS)
PRIVATE_KEY=your-ethereum-private-key-here
SEPOLIA_RPC=https://eth-sepolia.g.alchemy.com/v2/your-alchemy-key
IRYS_DEVNET_RPC=https://testnet-rpc.irys.xyz/v1/execution-rpc

# Frontend
REACT_APP_WALLETCONNECT_PROJECT_ID=your-walletconnect-project-id
REACT_APP_UMO_MOMENTS_CONTRACT=your-contract-address

# Security
ALLOWED_ORIGINS=http://localhost:3000,https://your-production-domain.com

# Production settings
NODE_ENV=production
PORT=5050